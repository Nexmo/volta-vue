    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-vue": "^4.0.0",
    "file-loader": "^1.1.11",
    "husky": "^1.0.0-rc.13",
    "jest": "^23.6.0",
    "jest-serializer-vue": "^2.0.2",
    "jsdom": "^13.1.0",
    "jsdom-global": "^3.0.2",
    "node-sass": "^4.12.0",
    "sass-loader": "^6.0.6",
    "storybook-addon-vue-info": "^1.0.1",
    "svg-sprite-loader": "^4.1.3",
    "vue-jest": "^3.0.4",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.3.1"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "<rootDir>/testSetup.js"
    ],
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      ".*\\.(js)$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/components/**/*.vue",
      "!**/node_modules/**"
    ],
    "snapshotSerializers": [
      "jest-serializer-vue"
    ]
  },
  "main": "src/components/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nexmo/volta-vue.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Nexmo/volta-vue/issues"
  },
  "homepage": "https://github.com/Nexmo/volta-vue#readme",
  "husky": {
    "hooks": {}
  }
}

  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "eslint": "^5.2.0",
    "eslint-config-airbnb-base": "^11.3.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-vue": "^4.0.0",
    "file-loader": "^1.1.11",
    "husky": "^1.0.0-rc.13",
    "node-sass": "^4.9.3",
    "sass-loader": "^6.0.6",
    "svg-sprite-loader": "^3.8.0",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  },
  "main": "src/components/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nexmo/volta-vue.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Nexmo/volta-vue/issues"
